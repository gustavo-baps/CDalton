{
    "manifest_version": 3,
    "name": "CDalton",
    "version": "1.3",
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "48": "/midia/icon128.png"
    },
    "permissions": ["scripting", "activeTab", "tabs"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["/js/content.js"]
      }
    ],
    "background": {
      "service_worker": "/js/background.js"
    }
  }
  